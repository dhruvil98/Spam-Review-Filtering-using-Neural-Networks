"        \n","    def fit(self, X, y=None):\n","        self.max_index = pad_sequences(X, maxlen=self.maxlen).max()\n","        return self\n","    \n","    def transform(self, X, y=None):\n","        X = pad_sequences(X, maxlen=self.maxlen)\n","        X[X > self.max_index] = 0\n","        return X\n","\n","padder = Padder(maxlen)"],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"EKEbPaQtBp81","colab_type":"code","outputId":"c177df65-2906-4760-f691-860c38444714","executionInfo":{"status":"ok","timestamp":1588171660368,"user_tz":-330,"elapsed":729,"user":{"displayName":"JAY SHAH","photoUrl":"","userId":"13504679010207240037"}},"colab":{"base_uri":"https://localhost:8080/","height":425}},"source":["object = Padder(2)\n","object.fit(a)\n","object.transform(a)"],"execution_count":0,"outputs":[{"output_type":"execute_result","data":{"text/plain":["array([[ 0,  8],\n","       [ 0,  2],\n","       [ 0,  1],\n","       [ 0,  3],\n","       [ 0,  0],\n","       [ 0,  4],\n","       [ 0,  5],\n","       [ 0,  9],\n","       [ 0,  0],\n","       [ 0,  1],\n","       [ 0,  3],\n","       [ 0,  6],\n","       [ 0,  1],\n","       [ 0, 10],\n","       [ 0,  0],\n","       [ 0,  6],\n","       [ 0,  2],\n","       [ 0,  1],\n","       [ 0,  0],\n","       [ 0, 11],\n","       [ 0,  5],\n","       [ 0,  7],\n","       [ 0,  7],\n","       [ 0,  4]], dtype=int32)"]},"metadata":{"tags":[]},"execution_count":12}]},{"cell_type":"code","metadata":{"id":"ruipwKIr0DUE","colab_type":"code","outputId":"43304436-ad70-4612-e1f0-f4f419a8ddc0","executionInfo":{"status":"ok","timestamp":1592055689422,"user_tz":-330,"elapsed":19020,"user":{"displayName":"JAY SHAH","photoUrl":"","userId":"13504679010207240037"}},"colab":{"base_uri":"https://localhost:8080/","height":615}},"source":["import numpy as np\n","from keras.models import Sequential\n","from keras.layers import Dense, Embedding, Bidirectional, CuDNNLSTM, Dropout\n","from keras.wrappers.scikit_learn import KerasClas